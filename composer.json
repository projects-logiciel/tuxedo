{
    "name": "tomirons/tuxedo",
    "description": "Simple transactional email classes/templates for Laravel 5.4 mailables",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Irons",
            "email": "tom.irons@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.4.*",
        "illuminate/mail": "5.4.*",
        "illuminate/support": "5.4.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "TomIrons\\Tuxedo\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
